@model IEnumerable<MuMerch.Models.BandManager>

@{
    ViewBag.Title = "BandManagerList";
    Layout = "~/Views/Shared/_Employee.cshtml";
}

<h2>BandManagerList</h2>

<p>
    @Html.ActionLink("Create New", "AddBandManager")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BandId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BandId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditBandManager", new { id=item.BandManagerId }) |
            @Html.ActionLink("Delete", "DeleteBandManager", new { id=item.BandManagerId })
        </td>
    </tr>
}

</table>
